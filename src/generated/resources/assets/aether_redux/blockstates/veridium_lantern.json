{
  "multipart": [
    {
      "apply": {
        "model": "aether_redux:block/veridium_lantern",
        "y": 90
      },
      "when": {
        "axis": "x",
        "hanging": "false"
      }
    },
    {
      "apply": {
        "model": "aether_redux:block/veridium_lantern"
      },
      "when": {
        "axis": "z",
        "hanging": "false"
      }
    },
    {
      "apply": {
        "model": "aether_redux:block/hanging_veridium_lantern",
        "y": 90
      },
      "when": {
        "axis": "x",
        "hanging": "true"
      }
    },
    {
      "apply": {
        "model": "aether_redux:block/hanging_veridium_lantern"
      },
      "when": {
        "axis": "z",
        "hanging": "true"
      }
    },
    {
      "apply": {
        "model": "aether_redux:block/hanging_veridium_lantern"
      },
      "when": {
        "hanging": "true"
      }
    }
  ]
}